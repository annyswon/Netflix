        "\n",
        "\n",
        "def time_score_cor(dataset):\n",
        "    time = dataset['Runtime'].to_numpy()\n",
        "    score = dataset['IMDB Score'].to_numpy()\n",
        "    return np.corrcoef(time, score)\n",
        "\n",
        "\n",
        "cor = time_score_cor(dataset)\n",
        "print(f'Runtime IMDB Score correlation is {cor}')\n",
        "\n"
